{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDB": {
    "ConnectionString": "mongodb://vhub-media-mongo-db:27017",
    "DatabaseName": "media",
    "TransactionsEnabled": true
  },
  "S3Options": {
    "ServiceURL": "http://minio-db:9000",
    "AccessKey": "minioadmin",
    "SecretKey": "minioadmin",
    "BucketName": "media",
    "Region": "us-east-1",
    "ForcePathStyle": true,
    "UseHttp": true
  },
  "ServiceOptions": {
    "ServiceName": "VHub.Media.Host"
  }
}
